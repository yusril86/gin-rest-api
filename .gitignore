.idea/
.vscode/
*.swp
*.bak
*~
.DS_Store

# Log files
*.log

# Build output and temporary files
tmp/
build/
dist/

# Environment variables
.env
.env.*

# Miscellaneous
coverage.out # Go test coverage profile

